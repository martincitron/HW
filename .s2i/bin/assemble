#!/bin/bash

echo "mv /tmp/src /var/ace-server/src/HW"

mv /tmp/src /var/ace-server/src/HW

echo "mqsipackagebar -w /var/ace-server/src -a /var/ace-server/src/HW.bar -k HW"

mqsipackagebar -w /var/ace-server/src -a /var/ace-server/src/HW.bar -k HW 

echo "mqsiapplybaroverrides -b /var/ace-server/src/HW.bar -p /var/ace-server/src/HW/overrides/HW_${ENV}.properties"

mqsiapplybaroverrides -b /var/ace-server/src/HW.bar -p /var/ace-server/src/HW/overrides/HW_${ENV}.properties

echo "mqsibar -w /var/ace-server/runtime -a /var/ace-server/src/HW.bar -c"

mqsibar -w /var/ace-server/runtime -a /var/ace-server/src/HW.bar -c

echo "cp -f /var/ace-server/src/HW/overrides /var/ace-server"

cp -f /var/ace-server/src/HW/overrides /var/ace-server/runtime
